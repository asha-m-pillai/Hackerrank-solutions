#include <bits/stdc++.h>

using namespace std;

int getTotalX(vector <int> a, vector <int> b) {
    // Complete this function
    int noOfElem = 0, i=2;
    bool isElem = true;
    for(int search = a[a.size()-1];search <= b[0];
               search = a[a.size()-1]*i,i++)
    {
        isElem = true;
        for(int j=0; j<a.size(); j++)
        {
            if(search%a[j] != 0)
            {
                isElem = false;
                break;
            }
        }
        if (isElem == false)
            continue;
        for(int j=0; j<b.size(); j++)
        {
            if(b[j]%search != 0)
            {
                isElem = false;
                break;
            }
        }
        if (isElem == false)
            continue;
        noOfElem++;
    }
    return noOfElem;
}

int main() {
    int n;
    int m;
    cin >> n >> m;
    vector<int> a(n);
    for(int a_i = 0; a_i < n; a_i++){
       cin >> a[a_i];
    }
    vector<int> b(m);
    for(int b_i = 0; b_i < m; b_i++){
       cin >> b[b_i];
    }
    int total = getTotalX(a, b);
    cout << total << endl;
    return 0;
}
